  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/vg-text.js",
      "require": "./dist/vg-text.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "browser": {
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
